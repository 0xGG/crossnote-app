  "scripts": {
    "git-info": "echo export default \"{\\\"logMessage\\\": \\\"$(git log -1 --oneline)\\\",\\\"hash\\\":\\\"$(git rev-parse HEAD)\\\"}\"  > src/_git_commit.js",
    "start": "yarn git-info; npm-run-all --parallel start:*",
    "start:react-scripts": "react-app-rewired start",
    "start:copy-css-files": "gulp copy-css-files",
    "build": "yarn git-info; yarn build:copy-css-files; yarn build:react-scripts; yarn build:service-worker",
    "build:react-scripts": "react-app-rewired build",
    "build:copy-css-files": "gulp copy-css-files",
    "build:service-worker": "gulp service-worker",
    "build:splash-screen": "cd public && npx pwa-asset-generator logo.svg ./assets -i ./index.html -m manifest.json",
    "test": "react-app-rewired test",
    "test:production": "cd build && http-server",
    "eject": "react-scripts eject",
    "dev:graphql-codegen": "graphql-codegen -c codegen.dev.yml",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "fix:prettier": "prettier --write \"**/*.*\"",
    "postinstall": "patch-package"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
